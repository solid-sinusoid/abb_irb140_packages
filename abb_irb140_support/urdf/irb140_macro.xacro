        <limit lower="-3.1416" upper="3.1416" effort="0" velocity="2.618"/>
    </joint>
    <joint name="${prefix}joint_2" type="revolute">
        <origin xyz="0.07 0 0.263" rpy="0 0 0"/>
        <parent link="${prefix}link_1"/>
        <child link="${prefix}link_2"/>
        <axis xyz="0 1 0"/>
        <limit lower="-1.5708" upper="1.91986" effort="0" velocity="2.618"/>
    </joint>
    <joint name="${prefix}joint_3" type="revolute">
        <origin xyz="0 0 0.36" rpy="0 0 0"/>
        <parent link="${prefix}link_2"/>
        <child link="${prefix}link_3"/>
        <axis xyz="0 1 0"/>
        <limit lower="-4.01426" upper="0.872665" effort="0" velocity="2.618"/>
    </joint>
    <joint name="${prefix}joint_4" type="revolute">
        <origin xyz="0.2185 0 0" rpy="0 0 0"/>
        <parent link="${prefix}link_3"/>
        <child link="${prefix}link_4"/>
        <axis xyz="1 0 0"/>
